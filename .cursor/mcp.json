{
    "mcpServers": {
        "task-master-ai": {
            "command": "npx",
            "args": [
                "-y",
                "task-master-mcp"
            ],
            "env": {
                "ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY",
                "PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY",
                "MODEL": "claude-3-7-sonnet-20250219",
                "PERPLEXITY_MODEL": "sonar-pro",
                "MAX_TOKENS": 64000,
                "TEMPERATURE": 0.2,
                "DEFAULT_SUBTASKS": 5,
                "DEFAULT_PRIORITY": "medium"
            }
        },
        "Modular-Health-NIPs Docs": {
            "url": "https://gitmcp.io/HealthNoteLabs/Modular-Health-NIPs"
        },
        "primal-android-app Docs": {
            "url": "https://gitmcp.io/PrimalHQ/primal-android-app"
        },
        "Runstr Docs": {
            "url": "https://gitmcp.io/HealthNoteLabs/Runstr"
        },
        "primal-server Docs": {
            "url": "https://gitmcp.io/PrimalHQ/primal-server"
        }
    }
}